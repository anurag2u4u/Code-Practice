#include<iostream>
using namespace std;
int swap(int&a,int&b){
    int temp=a;
    a=b;
    b=temp;
}
int main(){
    int x,y;
    cout<<"enter the value of a"<<endl;
    cin>>x;
    cout<<"enter the value of b"<<endl;
    cin>>y;
    swap(x,y);
    cout<<"now value of a is"<<x<<"now value of b is"<<y;
    return 0;
}
